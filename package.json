{
  "name": "portofolio-vite-yumana",
  "version": "1.0.1",
  "description": "Portofolio menggunakan vite, react dan chakra",
  "private": true,
  "author": "yumanauralfath",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc",
    "check:turbo": "pnpm dlx turbo@latest lint type-check",
    "knip": "knip",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/plus-jakarta-sans": "^5.0.19",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "framer-motion": "^11.0.14",
    "million": "3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/node": "^20.11.28",
    "@types/react": "18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "^6.1.11",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-sznm": "^2.0.3",
    "husky": "^9.0.11",
    "knip": "^5.1.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "standard-version": "^9.5.0",
    "typescript": "5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-pwa": "^0.19.4",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
  }
}
